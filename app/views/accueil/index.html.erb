<h1>Accueil#index</h1>
<p>Find me in app/views/accueil/index.html.erb</p>

<% if user_signed_in? %>
  <%= link_to "logout", destroy_user_session_path, :method => 'delete' %>
<% else %>
  <%= link_to "Login", new_user_session_path, :method => 'get' %>
<% end %>
<hr>
<div class="card">
  <img class="card-img-top" src="holder.js/100x180/" alt="">
  <div class="card-body">
    <h4 class="card-title"><%= current_user.email %></h4>
    <hr>
    <p class="card-text">
      <b>Provider: </b><%= current_user.provider %><br>
      <b>UID: </b><%= current_user.uid %>
    </p>
  </div>
</div>